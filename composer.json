{
  "name": "dosomething/mb-toolbox",
  "type": "library",
  "version": "0.11.2",
  "description": "A library of functionality shared between the components that make up the Quicksilver system: https://github.com/DoSomething/message-broker. Some of the functionality is DoSomething.org specific.",
  "keywords": ["rabbitmq", "message broker", "quicksilver", "php", "library"],
  "homepage": "https://github.com/DoSomething/mb-toolbox",
  "license" : "MIT",
  "authors": [
    {
      "name": "DoSomething and contributors",
      "homepage": "http://www.dosomething.org"
    }
  ],
  "require": {
    "php": ">= 5.3.0",
    "dosomething/mb-stat-tracker": "v0.0.3",
    "dosomething/stathat": "1.*",
    "dosomething/mobilecommons-php": "1.0.*",
    "drewm/mailchimp-api": "1.*",
    "richardfullmer/rabbitmq-management-api": "1.0.*"
  },
  "require-dev": {
    "phpunit/phpunit": "4.4.*"
  },
  "autoload": {
    "psr-4": {
      "DoSomething\\MB_Toolbox\\": "src"
    }
  }
}
